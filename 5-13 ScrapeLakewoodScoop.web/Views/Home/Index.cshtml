@using _5_13_ScrapeLakewoodScoop.api
@model IEnumerable<Article>

<div class="row">
    @foreach (var a in Model)
    {
     <div class="col-md-8 col-md-offset-3 well" >
         <h2><a href="@a.Url" target="_blank">@a.Title</a></h2>
         <br />
         <h5>@a.Date</h5>
         <br />
         <img src="@a.ImgUrl" />
         <br />
     </div>   
    }
</div>

